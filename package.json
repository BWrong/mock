{
  "name": "@bwrong/mock",
  "version": "0.0.10",
  "author": "bwrong<ibwrong@foxmail.com>",
  "license": "MIT",
  "keywords": [
    "mock",
    "webpack",
    "express"
  ],
  "publishConfig": {
    "access": "public"
  },
  "description": "简单实用的前端数据mock方案",
  "main": "src/index.js",
  "scripts": {
    "test:w": "nodemon ./tests/index.js",
    "test": "node ./tests/index.js",
    "pub": "npm publish",
    "commit": "git add . && git-cz",
    "release": "standard-version --infile CHANGELOG.md"
  },
  "homepage": "https://github.com/BWrong/mock.git",
  "bugs": {
    "url": "https://github.com/BWrong/mock.git/issues",
    "email": "ibwrong@foxmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BWrong/mock.git"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib/*",
    "src/*",
    "index.d.ts"
  ],
  "peerDependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "glob": "^7.1.6",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "express": "^4.17.1",
    "standard-version": "^9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": false
    }
  }
}
